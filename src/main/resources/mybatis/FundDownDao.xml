    <insert id="insertFundDown" parameterType="FundDownPo">
        INSERT INTO funddownload(code,ft, info) VALUES(#{code}, #{ft}, #{info})  ON DUPLICATE KEY UPDATE ft=VALUES(ft), info=VALUES(info)
    </insert>
    <insert id="insertFundBatch" parameterType="java.util.List">
        <selectKey resultType="java.lang.Integer" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into funddownload(code,ft, info) values
        <foreach collection="list" item="fundDownPo" index="index" separator=",">
            (
            #{fundDownPo.code}, #{fundDownPo.ft}, #{fundDownPo.info}
            )
        </foreach>
        ON DUPLICATE KEY UPDATE ft=VALUES(ft), info=VALUES(info)
    </insert>

