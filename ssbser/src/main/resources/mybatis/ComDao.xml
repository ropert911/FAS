    <insert id="insertCompBatch" parameterType="java.util.List">
        <selectKey resultType="java.lang.Integer" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into comp(ft,ordernum,name,estime,scale,fnnum,managernum) values
        <foreach collection="list" item="compPo" index="index" separator=",">
            (
            #{compPo.ft}, #{compPo.ordernum}, #{compPo.name},#{compPo.estime},#{compPo.scale},#{compPo.fnnum},
            #{compPo.managernum}
            )
        </foreach>
        ON DUPLICATE KEY UPDATE ft=VALUES(ft), ordernum=VALUES(ordernum), estime=VALUES(estime), scale=VALUES(scale),
        fnnum=VALUES(fnnum), managernum=VALUES(managernum)
    </insert>

