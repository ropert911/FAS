    <insert id="insertFundBatch" parameterType="java.util.List">
        <selectKey resultType="java.lang.Integer" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into fund(code,name,ft,date,l1y,l2y,l3y,ty,cy) values
        <foreach collection="list" item="fundPo" index="index" separator=",">
            (
            #{fundPo.code}, #{fundPo.name}, #{fundPo.ft},#{fundPo.date},#{fundPo.l1y}, #{fundPo.l2y},
            #{fundPo.l3y},#{fundPo.ty},#{fundPo.cy}
            )
        </foreach>
        ON DUPLICATE KEY UPDATE name=VALUES(name), ft=VALUES(ft), date=VALUES(date), l1y=VALUES(l1y), l2y=VALUES(l2y), l3y=VALUES(l3y),
        ty=VALUES(ty), cy=VALUES(cy)
    </insert>

